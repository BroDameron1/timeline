<% if (publicSourceData.mediaType === 'Movie') { %>
    <div class="data-line">
        Initial Release Date: <%= publicSourceData.movie.releaseDate %> 
    </div>
    <div class="data-line">
        Director(s):
        <% for (let director of publicSourceData.movie.director) { %>
            <div><%= director %></div>
        <% } %> 
    </div>
    <div class="data-line">
        Writer(s):
        <% for (let writer of publicSourceData.movie.writer) { %>
            <div><%= writer %></div>
        <% } %> 
    </div>
<% } %>


<% if (publicSourceData.mediaType === 'TV Show') { %>
    <div class="data-line">
        Initial Release Date: <%= publicSourceData.tv.releaseDate %> 
    </div>
    <div class="data-line">
        Series: <%= publicSourceData.tv.series %> 
    </div>
    <div class="data-line">
        Season: <%= publicSourceData.tv.season %>
    </div>
    <div class="data-line">
        Episode: <%= publicSourceData.tv.episode %>
    </div>
<% } %>



<% if (publicSourceData.mediaType === 'Book') { %>
    <div class="data-line">
        Initial Release Date: <%= publicSourceData.book.releaseDate %> 
    </div>
    <div class="data-line">
        Author(s):
        <% for (let author of publicSourceData.book.author) { %>
            <div><%= author %></div>
        <% } %> 
    </div>
    <div class="data-line">
        Series: <%= publicSourceData.book.series %> 
    </div>
    <div class="data-line">
        Publisher: <%= publicSourceData.book.publisher %> 
    </div>
    <div class="data-line">
        ISBN-10: <%= publicSourceData.book.isbn10 %> 
    </div>
<% } %>


<% if (publicSourceData.mediaType === 'Comic') { %>
    <div class="data-line">
        Initial Release Date: <%= publicSourceData.movie.releaseDate %> 
    </div>
    <div class="data-line">
        Writer: <%= publicSourceData.comic.writer %> 
    </div>
    <div class="data-line">
        Art Contributors:
        <% for (let artist of publicSourceData.comic.artContributor) { %>

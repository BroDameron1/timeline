    </div>
    <div class="data-line">
        Series: <%= publicSourceData.comic.series %> 
    </div>
    <div class="data-line">
        Issue Number: <%= publicSourceData.comic.issueNum %> 
    </div>
<% } %>


<% if (publicSourceData.mediaType === 'Video Game') { %>
